{
  "name": "lutex-ext",
  "displayName": "LuTeX",
  "description": "Jump to specific line numbers in LuTeX files based on network messages",
  "version": "0.0.1",
  "publisher": "xlu",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:latex"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lutex-ext.inlineToDisplay",
        "title": "Inline to display",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.bibtexClean",
        "title": "BibTeX Clean",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.texNormalization",
        "title": "Tex Normalization",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.launchRenderer",
        "title": "Launch Renderer",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.closeRenderer",
        "title": "Close Renderer",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.launchListener",
        "title": "Launch Listener",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.closeListener",
        "title": "Close Listener",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.launch",
        "title": "Launch (Both)",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.close",
        "title": "Close (Both)",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.jumpToHtml",
        "title": "Jump to HTML Element",
        "category": "LuTeX"
      }
    ],
    "configuration": {
      "title": "LuTeX",
      "properties": {
        "lutex-ext.rendererPort": {
          "type": "number",
          "default": 0,
          "description": "Port number for the LuTeX renderer server (0 = auto-detect)",
          "minimum": 0,
          "maximum": 65535
        },
        "lutex-ext.listenerPort": {
          "type": "number",
          "default": 0,
          "description": "Port number for the line jumping listener server (0 = auto-detect)",
          "minimum": 0,
          "maximum": 65535
        },
        "lutex-ext.theme": {
          "type": "string",
          "enum": ["light", "dark"],
          "default": "dark",
          "description": "Default theme for the LuTeX renderer"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helloluxi/lutex-ext"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vscode/vsce": "^2.15.0",
    "eslint": "^8.13.0",
    "typescript": "^5.9.2"
  }
}
