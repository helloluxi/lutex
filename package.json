{
  "name": "lutex-ext",
  "displayName": "LuTeX",
  "description": "Jump to specific line numbers in LuTeX files based on network messages",
  "version": "0.0.1",
  "publisher": "xlu",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lutex-ext.inlineToDisplay",
        "title": "Inline to display",
        "category": "LuTeX"
      },
      {
        "command": "lutex-ext.bibtexClean",
        "title": "BibTeX Clean",
        "category": "LuTeX"
      }
    ],
    "configuration": {
      "title": "LuTeX",
      "properties": {
        "lutex-ext.port": {
          "type": "number",
          "default": 1024,
          "description": "Port number for the HTTP server to listen on",
          "minimum": 1024,
          "maximum": 65535
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helloluxi/lutex-ext"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vscode/vsce": "^2.15.0",
    "eslint": "^8.13.0",
    "typescript": "^5.9.2"
  }
}
